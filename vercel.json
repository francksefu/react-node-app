{
    "version": 2,
    "builds": [
      { "src": "/index.mjs", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.mjs" }
    ],
    "headers": [
        {
        "source": "/(.*)",
        "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "http://localhost:3000" }
            
        ]
        }
    ]
  }