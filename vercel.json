{
    "version": 2,
    "builds": [
      { "src": "/index.mjs", "use": "@vercel/node" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "./index.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
  }